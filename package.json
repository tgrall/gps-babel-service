{
  "name": "gps-babel-service",
  "version": "1.2.1",
  "description": "This project expose GPS Babel as a service to convert trace to GPX format.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > ./src/version.ts",
    "build": "npx tsc ",
    "start": "node build/app.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q ./build/app.js\"",
    "lint": "eslint --ext .ts src/",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windr-app/gps-babel-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/windr-app/gps-babel-service/issues"
  },
  "homepage": "https://github.com/windr-app/gps-babel-service#readme",
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6",
    "path": "^0.12.7",
    "tmp": "^0.2.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.189",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "assert": "^2.0.0",
    "chai": "^4.3.7",
    "concurrently": "^7.5.0",
    "eslint": "^8.32.0",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
